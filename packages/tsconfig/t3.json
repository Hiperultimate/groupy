{
  "extends": "tsconfig/base.json",
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "T3",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "incremental": true,
    "noUncheckedIndexedAccess": true
  }
}
